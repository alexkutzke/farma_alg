aa = "<%= escape_javascript(render(:partial => 'tags_search_result', :locals => {:as => @as, :as_aat => @as_aat}))%>";

$("#search_result").html(aa);

$(".answer-btn").click(function(){
  $.ajax({
    url: "/dashboard/answers/show",
    data: {
      id: $(this).data("id")
    },
    type: "get"    
  });
});

$(".accept_tag_link").click(function(){
  
  var answer_id = $(this).data("answer-id");
  $.ajax({
    url: "/dashboard/tags/accept_tag",
    data: {
      id: selectedTagId,
      answer_id: answer_id
    },
    type: "put",
    success: function(){
      $("#"+answer_id).appendTo("#confirmed-tags")
      $("#"+answer_id+" button.accept_tag_link").remove()
      $("#"+answer_id+" button.reject_tag_link").remove()
    }
  });
});

$(".reject_tag_link").click(function(){
  
  var answer_id = $(this).data("answer-id");
  $.ajax({
    url: "/dashboard/tags/reject_tag",
    data: {
      id: selectedTagId,
      answer_id: answer_id
    },
    type: "put",
    success: function(){
      $("#"+answer_id).remove();
    }
  });
});

$("span.label").tooltip();
$("button[rel='tooltip']").tooltip();

if(searchResultCallback)
  searchResultCallback();