aa = "<%= escape_javascript(render(:partial => 'tags_search_result', :locals => {:as => @as, :total => @total}))%>";

$("form.search-bar").attr("action",$("form.search-bar").data("original-path"));

$("#search_result").html(aa);

$(".answer-btn").click(function(){
  $.ajax({
    url: "/dashboard/answers/show",
    data: {
      id: $(this).data("id")
    },
    type: "get"
  });
});

$("span.label").tooltip();
$("button[rel='tooltip']").tooltip();

if(searchResultCallback && typeof(searchResultCallback) == "function")
  searchResultCallback();
