<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Linha do Tempo
        <small>Analise as respostas ao longo do tempo</small>
    </h1>
    <ol class="breadcrumb">
        <li><i class="fa fa-home"></i> <%= link_to "Home", dashboard_home_path %></li>
        <li class="active"><i class="fa fa-clock-o"></i> Linha do Tempo</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <div class="row">
        <div style="margin-bottom:20px;" class="col-md-10 col-md-offset-1">
           <%= render(:partial => 'search_bar', :locals => { :form_id => "search-form", :path => dashboard_timeline_search_path }) %>
           <%= render(:partial => 'search_filters', :locals => { :form_id => "search-form", :users => @users, :teams => @teams, :los => @los, :questions => @questions, :tags => @tags }) %>
        </div>
    </div>

    <!-- Main row -->
    <div id="search_result">

    </div>

	<!-- row -->
	<div class="row">                        
	    <div class="col-md-12">
	        <!-- The time line -->
	        <ul class="timeline">
	            <!-- timeline time label -->
	            <li class="time-label">
	                <span class="bg-red">
	                    10 Feb. 2014
	                </span>
	            </li>
	            <!-- /.timeline-label -->
	            <!-- timeline item -->
	            <li>
	                <i class="fa fa-envelope bg-blue"></i>
	                <div class="timeline-item">
	                    <span class="time"><i class="fa fa-clock-o"></i> 12:05</span>
	                    <h3 class="timeline-header"><a href="#">Support Team</a> sent you and email</h3>
	                    <div class="timeline-body">
	                        Etsy doostang zoodles disqus groupon greplin oooj voxy zoodles,
	                        weebly ning heekya handango imeem plugg dopplr jibjab, movity
	                        jajah plickers sifteo edmodo ifttt zimbra. Babblely odeo kaboodle
	                        quora plaxo ideeli hulu weebly balihoo...
	                    </div>
	                    <div class='timeline-footer'>
	                        <a class="btn btn-primary btn-xs">Read more</a>
	                        <a class="btn btn-danger btn-xs">Delete</a>
	                    </div>
	                </div>
	            </li>
	            
	            <li>
	                <i class="fa fa-clock-o"></i>
	            </li>
	        </ul>
	    </div><!-- /.col -->
	</div><!-- /.row -->
</section><!-- /.content -->