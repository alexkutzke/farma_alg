$("#timelinee").html("<%= escape_javascript(render(:partial => 'timeline_search_result', :locals => {:as => @as,:timeline_items => @timeline_items}))%>");

$(".answer-btn").click(function(){
  $.ajax({
    url: "/dashboard/answers/show",
    data: {
      id: $(this).data("id")
    },
    type: "get"    
  });
});

$(".connection-btn").click(function(){
  $.ajax({
    url: "/dashboard/connections/show",
    data: {
      id: $(this).data("id")
    },
    type: "get"    
  });
});


$("span.label").tooltip();
$("button[rel='tooltip']").tooltip();

$(".time-label-btn").click(function(){
  date = $(this).data("date");
  $("."+date).toggle();
});

if(searchResultCallback)
  searchResultCallback();