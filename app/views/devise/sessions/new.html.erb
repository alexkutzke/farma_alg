<%= form_for(resource, :as => resource_name, :url => session_path(resource_name),:class=>"form-signin") do |f| %>
  <div><%= f.label :email %>
  <%= f.email_field :email, :class=>"form-control" %></div>

  <div><%= f.label :password, "Senha" %>
  <%= f.password_field :password, :class=>"form-control" %></div>

    <div class="form-actions"><%= f.submit "Entrar", :class => "btn btn-lg btn-primary" %></div>
<% end %>

<%= render "devise/shared/links" %>

